{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": 1,
  "links": [],
  "panels": [
    {
      "title": "System Health Overview",
      "type": "row",
      "panels": [
        {
          "title": "Memory Integrity",
          "type": "gauge",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "phoenix_memory_integrity",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "min": 0,
              "max": 1,
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": 0 },
                  { "color": "yellow", "value": 0.9 },
                  { "color": "green", "value": 0.95 }
                ]
              }
            }
          }
        },
        {
          "title": "Conscience Alignment",
          "type": "gauge",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "phoenix_conscience_alignment",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "min": 0,
              "max": 1,
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": 0 },
                  { "color": "yellow", "value": 0.9 },
                  { "color": "green", "value": 0.95 }
                ]
              }
            }
          }
        },
        {
          "title": "Value Drift",
          "type": "gauge",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "phoenix_value_drift",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "min": 0,
              "max": 1,
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "green", "value": 0 },
                  { "color": "yellow", "value": 0.2 },
                  { "color": "red", "value": 0.3 }
                ]
              }
            }
          }
        }
      ]
    },
    {
      "title": "Learning & Performance",
      "type": "row",
      "panels": [
        {
          "title": "Learning Rate",
          "type": "graph",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "rate(phoenix_learning_rate[5m])",
              "refId": "A"
            }
          ]
        },
        {
          "title": "Memory Usage",
          "type": "graph",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "phoenix_memory_size_bytes",
              "refId": "A"
            }
          ]
        },
        {
          "title": "Error Rate",
          "type": "graph",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "rate(phoenix_errors_total[5m])",
              "refId": "A"
            }
          ]
        }
      ]
    },
    {
      "title": "Component Status",
      "type": "row",
      "panels": [
        {
          "title": "Component Health",
          "type": "stat",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "up{job=~\"phoenix.*\"}",
              "refId": "A"
            }
          ]
        },
        {
          "title": "Component Latency",
          "type": "graph",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "phoenix_component_latency_ms",
              "refId": "A"
            }
          ]
        }
      ]
    },
    {
      "title": "Safety Metrics",
      "type": "row",
      "panels": [
        {
          "title": "Safety Violations",
          "type": "stat",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "sum(increase(phoenix_safety_violations_total[24h]))",
              "refId": "A"
            }
          ]
        },
        {
          "title": "Emergency Shutdowns",
          "type": "stat",
          "datasource": "Prometheus",
          "targets": [
            {
              "expr": "sum(increase(phoenix_emergency_shutdowns_total[24h]))",
              "refId": "A"
            }
          ]
        }
      ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 27,
  "style": "dark",
  "tags": ["phoenix", "agi"],
