{
  "name": "phoenix-orch-ashen-guard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5000 --strictPort",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-directives --max-warnings 0",
    "preview": "vite preview --port 5000",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "test:performance": "vitest --config vitest.performance.config.ts",
    "test:performance:run": "vitest run --config vitest.performance.config.ts",
    "test:performance:coverage": "vitest run --config vitest.performance.config.ts --coverage",
    "test:neural-emotion": "vitest run --config vitest.performance.config.ts tests/performance/neural_emotion_integration.test.ts",
    "test:neural-emotion:watch": "vitest --config vitest.performance.config.ts tests/performance/neural_emotion_integration.test.ts",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:report": "playwright show-report"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@tanstack/react-query": "^5.90.11",
    "@tauri-apps/api": "^1.5.0",
    "@types/lodash": "^4.17.21",
    "@types/mousetrap": "^1.6.15",
    "autoprefixer": "^10.4.22",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "eventsource-parser": "^3.0.6",
    "framer-motion": "^12.23.24",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.284.0",
    "monaco-editor": "^0.55.1",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1",
    "tailwindcss": "^4.1.17",
    "three": "^0.181.2",
    "ws": "^8.18.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.32",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
